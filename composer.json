{
    "name": "marcinjean/minisnowflake",
    "description": "A lightweight, case-insensitive, short ID generator using a mini Snowflake algorithm.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/MarcinJean/MiniSnowFlake",
    "autoload": {
        "psr-4": {
            "MarcinJean\\MiniSnowflake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcinJean\\MiniSnowflake\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "phpunit"
    },
    "bin": [
        "bin/generate-id.php"
    ],
    "authors": [
        {
            "name": "Marcin Jean",
            "email": "Marcin@MarcinJean.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "MarcinJean\\MiniSnowflake\\Providers\\LaravelServiceProvider"
            ]
        }
    }
}
