{
    "name": "marcinjean/minisnowflake",
    "description": "A lightweight, case-insensitive, short ID generator using a mini Snowflake algorithm.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MiniSnowflake\\IdGenerator\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "MiniSnowflake\\IdGenerator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "authors": [
        {
            "name": "Marcin Jean",
            "email": "Marcin@MarcinJean.com"
        }
    ],
    "minimum-stability": "stable"
}
